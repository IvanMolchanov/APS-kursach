<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" minHeight="400.0" minWidth="900.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="spbstu.ru.molchanovIr.kursach.ApplicationController">
    <TableView fx:id="tableResults" prefHeight="324.0" prefWidth="860.0">
    </TableView>
    <Button fx:id="startButton" onAction="#onStartButtonClick" text="start"/>
    <Button fx:id="stepButton" onAction="#onStepButtonClick" text="next step"/>
    <Button fx:id="continueButton" onAction="#onContinueButtonClick" text="continue"/>
</VBox>
